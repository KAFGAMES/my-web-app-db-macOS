# Gemfile

source "https://rubygems.org"

gem "jekyll", "~> 3.10" # Jekyllのバージョン指定
gem "github-pages", group: :jekyll_plugins # GitHub Pages用のGem

# 必要に応じて追加するプラグイン
gem "jekyll-seo-tag" # SEOタグ用のプラグイン
gem "jekyll-paginate" # ページネーション用のプラグイン
gem "jekyll-sitemap" # サイトマップ生成用のプラグイン

# 他にも必要なプラグインがあればここに追加

gem 'fiddle'

